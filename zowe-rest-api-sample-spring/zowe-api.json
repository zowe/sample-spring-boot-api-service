{
    "zosSourcesDir": "zossrc",
    "buildCommand": "make; make install",
    "buildFiles": {
        "libwtojni.so": "src/main/resources/lib/libwtojni.so"
    },
    "deployFiles": {
        "build/libs/zowe-rest-api-sample-spring-0.0.1-SNAPSHOT.jar": {
            "target": "jars/zowe-rest-api-sample-spring.jar",
            "postCommands": [
                "java -cp jars/zowe-rest-api-sample-spring.jar -Dloader.main=org.zowe.sample.apiservice.LibsExtractor org.springframework.boot.loader.PropertiesLauncher .",
                "extattr +p *.so"
            ],
            "binary": true
        }
    },
    "shellStartCommand": "java -Xquickstart -jar jars/zowe-rest-api-sample-spring.jar --spring.config.additional-location=file:config/application.yml"
}
