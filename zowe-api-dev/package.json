{
  "name": "@zowedev/zowe-api-dev",
  "description": "Zowe REST API Development CLI",
  "version": "0.0.0-dev",
  "author": "zowe",
  "bin": {
    "zowe-api-dev": "./bin/run"
  },
  "bugs": "https://github.com/zowe/sample-spring-boot-api-service/issues",
  "dependencies": {
    "@oclif/command": "^1.5.18",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "debug": "^4.1.1",
    "handlebars": "^4.1.2",
    "log-symbols": "^3.0.0",
    "tmp": "^0.1.0",
    "tslib": "^1.10.0"
  },
  "peerDependencies": {
    "@zowe/cli": "^6.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.2.0",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.17",
    "@types/tmp": "^0.1.0",
    "chai": "^4.2.0",
    "globby": "^10.0.1",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "prettier": "1.18.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/zowe/sample-spring-boot-api-service/zowe-rest-api-commons-cli",
  "keywords": [
    "oclif"
  ],
  "license": "EPL-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "zowe-api-dev",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "zowe/sample-spring-boot-api-service",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "prettier": "npx prettier --write \"src/**/*.ts\"",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
